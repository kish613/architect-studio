# Database (Neon PostgreSQL)
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Google Gemini API (for image generation)
GOOGLE_GEMINI_API_KEY=your-gemini-api-key

# Meshy 3D API
MESHY_API_KEY=your-meshy-api-key

# Stripe
STRIPE_SECRET_KEY=sk_test_...
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Session
SESSION_SECRET=your-random-session-secret-at-least-32-characters

# Hugging Face (for TRELLIS 3D generation)
HF_TOKEN=hf_your-hugging-face-token
# TRELLIS Space (optional — defaults to the community Space)
# Set this to your own duplicated Space for a dedicated GPU, e.g. your-username/TRELLIS
TRELLIS_SPACE=trellis-community/TRELLIS

# Vercel Blob (auto-configured on Vercel)
BLOB_READ_WRITE_TOKEN=vercel_blob_...

# EPC Register API — no key required (free public API)

# Perplexity API (for real planning permission search)
PERPLEXITY_API_KEY=pplx-your-perplexity-api-key


